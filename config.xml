<?xml version='1.0' encoding='utf-8'?>
<widget id="org.michenux.mobilefirst.websdk" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:mfp="http://www.ibm.com/mobilefirst/cordova-plugin-mfp">
    <name>Test MobileFirst WebSDK</name>
    <description>Sample application to test WebSDK.</description>
    <author email="developer@yourcompany.com" href="https://yourcompany.com">
        Michenux
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-splashscreen" spec="~3" />
    <plugin name="cordova-plugin-whitelist" spec="~1" />
    <preference name="windows-target-version" value="10.0" />
    <preference name="windows-phone-target-version" value="10.0" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <engine name="android" spec="~6" />
    <engine name="windows" spec="4.4.3" />
</widget>
